<Window x:Class="MapEdit.OpenCloudMapSet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="OpenCloudMapSet" Height="350" Width="565" WindowStartupLocation="CenterOwner">
	<Grid>
		<StackPanel>
		<DataGrid x:Name="mapSetDataGrid" Margin="0,10,0,10" VerticalAlignment="Top" Height="192" MouseDoubleClick="MapSetDataGrid_OnMouseDoubleClick"/>
			<StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
				<Button Name="SelectButton" Click="SelectButton_OnClick" Margin="0,0,5,0" Width="120">Open Selected</Button>
				<Button Name="CancelButton" Click="CancelButton_OnClick" Margin="5,0,0,0" Width="120">Cancel</Button>
			</StackPanel>
		</StackPanel>
	</Grid>
</Window>
