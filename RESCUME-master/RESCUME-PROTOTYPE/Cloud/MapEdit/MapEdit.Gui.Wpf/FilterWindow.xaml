<Window x:Class="MapEdit.FilterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FilterWindow" Height="140" Width="300" WindowStartupLocation="CenterOwner">
	<StackPanel HorizontalAlignment="Center">
		
		<StackPanel Orientation="Horizontal" Margin="0,10,10,0">
			<Label x:Name="minValue">10</Label>
			<Slider x:Name="Slider"  Minimum="10" Maximum="100" IsSnapToTickEnabled="True" TickFrequency="10" SmallChange="10" LargeChange="10" HorizontalAlignment="Left" Margin="10,0,10,0" Width="200" VerticalAlignment="Top" ValueChanged="RangeBase_OnValueChanged"/>
			<Label x:Name="maxValue">100</Label>
		</StackPanel>
		<Label x:Name="CurrentValuelLabel" HorizontalAlignment="Center" Margin="0,0,0,10">.</Label>
		<Button Width="80" Click="ButtonBase_OnClick">Close</Button>
	</StackPanel>
</Window>
