<Window x:Class="MapEdit.DeleteAllMapData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DeleteAllMapData" Height="160" Width="300" WindowStartupLocation="CenterOwner">
	<StackPanel>
		<Label x:Name="warningLabel" Content="Delete Selected Cloud Map Data?" HorizontalAlignment="Center" Margin="0,20,0,20" VerticalAlignment="Top"  IsEnabled="False" />
		<StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
			<Button x:Name="overwriteOK" Content="OK" HorizontalAlignment="Left" Margin="0,0,5,0" VerticalAlignment="Top" Width="75" Click="overwriteOK_Click" IsDefault='True'/>
			<Button x:Name="overwriteCancel" Content="Cancel" HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Top" Width="75" Click="overwriteCancel_Click" IsCancel='True'/>
		</StackPanel>
	</StackPanel>
</Window>
