ifndef TGT
TGT:=
endif
ifndef LIB
LIB:=libASN_r36.so
endif


ifndef SAVARI_CXX
SAVARI_CXX=g++ 
endif
ifndef SAVARI_LD
SAVARI_LD=g++ 
endif
ifndef CXXFLAGS
CXXFLAGS:=-fPIC
endif
ifndef LDFLAGS
LDFLAGS:=
endif
ifndef INCLUDES
INCLUDES:=-I. 
endif
ifndef STRIP
STRIP:=strip
endif
ifndef LIBS
LIBS =
endif

override LDFLAGS  += 
#override APP_LIBS += -leloop -pthread

OBJ = $(patsubst %.cpp, %.o, ${SRC})

LIBS +=
all:	$(LIB) $(TGT)


$(LIB):	$(OBJ)
	${SAVARI_CXX} -shared $(OBJ)  -o $(LIB) $(APP_LIBS) $(LDFLAGS)
#$(TGT):	$(OBJ)
	#${SAVARI_LD} $(LDFLAGS) $(OBJ) -o $(TGT) $(LIB) $(APP_LIBS)

%.o: %.cpp
	${SAVARI_CXX} -g $(INCLUDES) $(CXXFLAGS) -c -o $@ $<

clean:
	-rm -f $(TGT) $(OBJ) $(LIB) $(OBJSRC) *~ *.o
